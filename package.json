{
  "private": true,
  "main": "main/index.js",
  "name": "timing_chart_editor",
  "version": "0.0.1",
  "productName": "ElectronTypescriptNext",
  "scripts": {
    "clean": "rimraf dist main renderer/out renderer/.next",
    "dev": "npm run build-electron && electron .",
    "build-renderer": "next build renderer",
    "build-electron": "tsc -p electron-src",
    "build": "npm run clean && npm run build-renderer && npm run build-electron ",
    "build-win": "npm run clean && npm run build-renderer && npm run build-electron --win --x64",
    "pack-app": "npm run build && electron-builder --dir",
    "pack-app-win": "npm run build-win && electron-builder --dir",
    "dist": "npm run build && electron-builder",
    "type-check": "tsc -p ./renderer/tsconfig.json && tsc -p ./electron-src/tsconfig.json"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/icons-material": "^5.15.12",
    "@mui/material": "^5.15.15",
    "canvas": "^3.2.0",
    "electron-is-dev": "^1.2.0",
    "electron-next": "^3.1.5",
    "konva": "^9.3.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "styled-components": "^6.1.8"
  },
  "devDependencies": {
    "@types/node": "^20.11.25",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "electron": "^39.2.4",
    "electron-builder": "^24.9.1",
    "next": "latest",
    "npm-check-updates": "^16.14.15",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  },
  "build": {
    "asar": true,
    "files": [
      "!renderer/context.tsx",
      "!renderer/pages/editor/*",
      "!renderer/pages/editor/components/*",
      "!renderer/pages/editor/components"
    ]
  },
  "volta": {
    "node": "20.9.0",
    "npm": "10.5.0"
  }
}
